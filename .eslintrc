{
  "env": {
      "browser": true,
      "es6": true,
      "node": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
      "ecmaFeatures": {
          "experimentalObjectRestSpread": true,
          "jsx": true
      },
      "sourceType": "module"
  },
  "plugins": [
      "prettier"
  ],
  "rules": {
    "no-console": "warn",
    "prettier/prettier": "error"
  }
}
